<resources>

    <!-- Base application theme. -->
    <style name="GDPR_AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorGdprPrimary</item>
        <item name="colorPrimaryDark">@color/colorGdprPrimaryDark</item>
        <item name="colorAccent">@color/colorGdprAccent</item>
    </style>

    <style name="GDPR_AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />

</resources>